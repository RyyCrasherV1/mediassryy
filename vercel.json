{
  "version": 2,
  "routes": [
    {
      "src": "/api/messages",
      "dest": "/api/messages.js"
    },
    {
      "src": "/api/send",
      "dest": "/api/send.js"
    },
    {
      "src": "/api/admin",
      "dest": "/api/admin.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ]
}
